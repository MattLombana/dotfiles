##########     Other Aliases      ##########
alias c='clear'
alias cd..='cd ..'
alias ..='cd ..'
alias home='cd ~/..'
alias main='cd ~'
alias mian='cd ~'
alias back='cd -'
alias user='echo $USER'
alias todo='~/Program-Files/todo.txt_cli-2.9/todo.sh'
complete -F _todo todo

# some more ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias ls-al='ls -al'
alias lsl='ls -l'
alias lsa='ls -a'
alias lsal='ls -al'
alias lsla='ls -al'
